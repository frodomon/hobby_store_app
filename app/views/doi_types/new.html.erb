<% provide(:title, "El Tablero de Otto | Nuevo Tipo de Documento de Identidad") %>
<div class="row wrapper border-bottom white-bg page-heading breadcrumbs-margin">
  <div class="col-lg-12">
    <h2>Nuevo Tipo de Documento de Identidad</h2>
    <ol class="breadcrumb">
      <li>
        <a href="<%= url_for :controller => 'static_pages', :action => 'home' %>">Inicio</a>
      </li>
      <li>
        <a href="<%= url_for :controller => 'doi_types', :action => 'index' %>">Tipo de Documento de Identidad</a>
      </li>
      <li class="active">
        <strong>Nuevo Tipo de Documento de Identidad</strong>
      </li>
    </ol>
  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight no-padding">
  <%= render 'shared/notifications' %>
  <div class="row breadcrumbs-margin">
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h3>Nuevo Tipo de Documento de Identidad</h3>
        </div>
        <div class="ibox-content">
            <%= render 'form' %>
            <%= button_to 'Volver', { action: 'index'}, method: :get, class: 'btn btn-w-m btn-info' %>
        </div>
      </div>
    </div>
  </div>  
</div>