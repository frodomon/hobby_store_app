<p id="notice"><%= notice %></p>

<h1>Suggested Ages</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suggested_ages.each do |suggested_age| %>
      <tr>
        <td><%= suggested_age.name %></td>
        <td><%= link_to 'Show', suggested_age %></td>
        <td><%= link_to 'Edit', edit_suggested_age_path(suggested_age) %></td>
        <td><%= link_to 'Destroy', suggested_age, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Suggested Age', new_suggested_age_path %>
