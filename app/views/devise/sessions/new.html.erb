<%= form_for(resource, as: resource_name, url: session_path(resource_name), class:'m-t' ) do |f| %>
  <div class="form-group">
    <%= f.email_field :email, autofocus: true, placeholder: 'Correo Electrónico',autocomplete: "email", class:'form-control' %>
  </div>
  <div class="form-group">
    <%= f.password_field :password, autocomplete: "off", class:'form-control' %>
  </div>


  <div class="actions">
    <%= f.submit "Iniciar Sesión", class: "btn btn-success block full-width m-b" %>
  </div>
<% end %>
<%= render 'shared/login_notifications' %>
<%= render "devise/shared/links" %>